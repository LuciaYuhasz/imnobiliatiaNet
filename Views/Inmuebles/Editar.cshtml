@model imnobiliatiaNet.Models.Inmueble

@{
    ViewData["Title"] = "Editar Inmueble";
}

<div class="container mt-5">
    <div class="row justify-content-center">
        <div class="col-md-12">
            <fieldset class="border p-4 rounded shadow text-white"
                style="background-color: rgba(116, 116, 117, 0.658);">
                <legend class="text-center mb-4 display-6 fw-bold fst-normal" style="letter-spacing: 1px;">
                    Editar Inmueble
                </legend>

                <form asp-action="Editar" method="post">
                    <input type="hidden" asp-for="Id" />
                    <partial name="_Form" />
                    <div class="text-center mt-4">
                        <button type="submit" class="btn btn-secondary btn-lg">Guardar</button>
                        <a asp-action="Index" class="btn btn-light btn-lg ms-2">Volver</a>
                    </div>
                </form>
            </fieldset>
        </div>
    </div>
</div>
