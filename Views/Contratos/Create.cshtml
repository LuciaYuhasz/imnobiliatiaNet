@model imnobiliatiaNet.Models.Contrato
@{
    ViewData["Title"] = "Nuevo Contrato";
}

<h2>@ViewData["Title"]</h2>

@if (TempData["Error"] != null)
{
    <div class="alert alert-danger text-center fw-semibold">
        @TempData["Error"]
    </div>
}

<form asp-action="Create">
    <partial name="_Form" />
</form>